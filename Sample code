stock DeleteMetroMapTextdraw(playerid)
{
        CancelSelectTextDraw(playerid);
        for(new i; i < sizeof(metro_map_PTD[]); i++)
                PlayerTextDrawDestroy(playerid, metro_map_PTD[playerid][i]);
}

stock CreateMetroStopTextdraw(playerid)
{
        metro_stop_PTD[playerid][0] = CreatePlayerTextDraw(playerid, 11.0000, 344.0000, "Box"); // пусто
        PlayerTextDrawLetterSize(playerid, metro_stop_PTD[playerid][0], 0.0000, 10.0000);
        PlayerTextDrawTextSize(playerid, metro_stop_PTD[playerid][0], 197.0000, 0.0000);
        PlayerTextDrawAlignment(playerid, metro_stop_PTD[playerid][0], 1);
        PlayerTextDrawColor(playerid, metro_stop_PTD[playerid][0], -1);
        PlayerTextDrawUseBox(playerid, metro_stop_PTD[playerid][0], 1);
        PlayerTextDrawBoxColor(playerid, metro_stop_PTD[playerid][0], -801552641);
        PlayerTextDrawBackgroundColor(playerid, metro_stop_PTD[playerid][0], 255);
        PlayerTextDrawFont(playerid, metro_stop_PTD[playerid][0], 1);
        PlayerTextDrawSetProportional(playerid, metro_stop_PTD[playerid][0], 1);
        PlayerTextDrawSetShadow(playerid, metro_stop_PTD[playerid][0], 0);

        metro_stop_PTD[playerid][1] = CreatePlayerTextDraw(playerid, 12.0000, 345.0000, "LD_SPAC:white"); // пусто
        PlayerTextDrawTextSize(playerid, metro_stop_PTD[playerid][1], 184.0000, 87.0000);
        PlayerTextDrawAlignment(playerid, metro_stop_PTD[playerid][1], 1);
        PlayerTextDrawColor(playerid, metro_stop_PTD[playerid][1], 255);
        PlayerTextDrawBackgroundColor(playerid, metro_stop_PTD[playerid][1], 255);
        PlayerTextDrawFont(playerid, metro_stop_PTD[playerid][1], 4);
        PlayerTextDrawSetProportional(playerid, metro_stop_PTD[playerid][1], 0);
        PlayerTextDrawSetShadow(playerid, metro_stop_PTD[playerid][1], 0);

        metro_stop_PTD[playerid][2] = CreatePlayerTextDraw(playerid, 11.0000, 362.0000, "LD_SPAC:white"); // пусто
