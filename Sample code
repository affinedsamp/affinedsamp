define MAX_METRO_STATIONS                      13
#define METRO_TIME_STOP                         10000
#define METRO_TIME_DELAY                        30000
#define METRO_TICKET_PRICE                      50

static const Float:metro_enter_pos[MAX_METRO_STATIONS][3] = {
        {1810.8765, -1877.1888, 13.5839}, // Unity
        {1585.6698, -2286.6060, 13.5396}, // Airport
        {1416.1969, -1662.7063, 13.5469}, // Pershing Square
        {937.9420, -1736.9058, 13.5469}, // Verona Beach
        {477.3788, -1762.0093, 5.5290}, // Santa Maria Beach
        {575.7296, -1419.6672, 14.4687}, // Rodeo
        {1220.0338, -1428.4200, 13.3714}, // Market Square
        {1544.1118, -1151.7020, 24.0703}, // Commercial Area
        {1831.7992, -1302.0525, 13.6033}, // Glen Park
        {2235.8618, -1125.7218, 25.7969}, // Jefferson
        {2521.3247, -1300.5614, 34.8516}, // East Side
        {2652.5598, -1628.5577, 10.8807}, // LS Forum
        {2323.3357, -1956.5929, 13.5699} // Willowfield
};
static const Float:metro_exit_pos[MAX_METRO_STATIONS][4] = {
        {1813.4556, -1877.1926, 13.5704, 270.00}, // Unity
        {1583.5385, -2286.5608, 13.5396, 90.00}, // Airport
        {1417.9331, -1662.7446, 13.5469, 270.00}, // Pershing Square
        {935.7811, -1736.8323, 13.5469, 90.00}, // Verona Beach
        {477.3500, -1764.1151, 5.5333, 190.00}, // Santa Maria Beach
        {575.2502, -1417.6353, 14.4702, 11.00}, // Rodeo
        {1220.0323, -1426.2465, 13.3380, 0.00}, // Market Square
        {1544.2295, -1154.1808, 24.0781, 180.00}, // Commercial Area
        {1834.3058, -1302.1033, 13.5979, 270.00}, // Glen Park
        {2234.6318, -1127.9963, 25.7969, 150.00}, // Jefferson
        {2518.2205, -1300.5613, 34.8516, 90.00}, // East Side
        {2649.9888, -1628.5851, 10.8807, 90.00}, // LS Forum
        {2321.2300, -1956.6156, 13.5662, 90.00} // Willowfield
};
static const metro_name[MAX_METRO_STATIONS][] = {
        "Unity",
        "Airport",
        "Pershing Square",
        "Verona Beach",
        "Santa Maria Beach",
        "Rodeo",
        "Market Square",
        "Commercial Area",
        "Glen Park",
        "Jefferson",
        "East Side",
        "Los Santos Forum",
        "Willowfield"
};
new metro_pickup[MAX_METRO_STATIONS];
new metro_timer[MAX_PLAYERS];
new PlayerText:metro_map_PTD[MAX_PLAYERS][31];
new PlayerText:metro_stop_PTD[MAX_PLAYERS][9];

hook OnGameModeInit()
{
        LoadMapping();
        for(new i; i < MAX_METRO_STATIONS; i++)
        {
                metro_pickup[i] = CreateDynamicPickup(19132, 23,
                        metro_enter_pos[i][0], metro_enter_pos[i][1], metro_enter_pos[i][2]-0.3,
                        0, 0, -1, 100.0);
                CreateDynamicMapIcon(
                        metro_enter_pos[i][0], metro_enter_pos[i][1], metro_enter_pos[i][2],
                        56, 0, 0, 0, -1, 100.0);
                new string[95];
                format(string, sizeof(string), "Станция метро \" %s \"\n\
                        {FFFFFF}Для взаимодействия нажите {FFA436}\" ALT \"", metro_name[i]);
                CreateDynamic3DTextLabel(string, 0xFFA436FF, 
                        metro_enter_pos[i][0], metro_enter_pos[i][1], metro_enter_pos[i][2]+0.6, 
                        13.0, INVALID_PLAYER_ID, INVALID_PLAYER_ID, 1, 0, 0, -1, 100.0);
        }
}

hook OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
        OnPlayerEnterMetro(playerid, newkeys);
        OnPlayerLeaveMetro(playerid, newkeys);
}

hook OnPlayerClickPlayerTextDraw(playerid, PlayerText:playertextid)
{
        OnPlayerClickOnMetroMap(playerid, playertextid);
}

hook OnPlayerClickTextDraw(playerid, Text:clickedid)
{
        if(GetPVarInt(playerid, "PlayerSelectTD") == 1)
        {
                if(_:clickedid == INVALID_TEXT_DRAW) SelectTextDraw(playerid, 0xFFFFFF16);
        }
}

stock OnPlayerEnterMetro(playerid, newkeys)
{
        if(newkeys & KEY_WALK)
        {
                for(new i; i < MAX_METRO_STATIONS; i++)
                {
                        if(IsPlayerInRangeOfPoint(playerid, 1.5, 
                                metro_enter_pos[i][0], 
                                metro_enter_pos[i][1],
                                metro_enter_pos[i][2]))
                        {
                                CreateMetroMapTextdraw(playerid);
                                PlayerTextDrawSetSelectable(playerid, metro_map_PTD[playerid][i+3], false);
                                CreateMetroStopTextdraw(playerid);
                                PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][3], metro_name[i]);
                                SelectTextDraw(playerid, 0xFFFFFF16);
                                SetPVarInt(playerid, "MetroStation", i+1);
                                SetPVarInt(playerid, "PlayerSelectTD", 1);
                                for(new a; a < sizeof(metro_map_PTD[]); a++)
                                        PlayerTextDrawShow(playerid, metro_map_PTD[playerid][a]);
                        }
                }
        }
}

stock OnPlayerLeaveMetro(playerid, newkeys)
{
        if(newkeys & KEY_NO)
        {
                if(GetPVarInt(playerid, "MetroStop") == 1)
                {
                        new metro_station_num = GetPVarInt(playerid, "MetroStation");
                        SetPlayerPos(playerid, 
                                metro_exit_pos[metro_station_num-1][0], 
                                metro_exit_pos[metro_station_num-1][1], 
                                metro_exit_pos[metro_station_num-1][2]);
                        SetPlayerVirtualWorld(playerid, 0);
                        SetPlayerFacingAngle(playerid, metro_exit_pos[metro_station_num-1][3]);
                        ClearAnimations(playerid);
                        TogglePlayerControllable(playerid, true);
                        SetCameraBehindPlayer(playerid);
                        DeleteMetroStopTextdraw(playerid);
                        KillTimer(metro_timer[playerid]);
                        metro_timer[playerid] = -1;
                        metro_station_num = -1;
                        DeletePVar(playerid, "MetroStop");
                        DeletePVar(playerid, "MetroStation");
                        DeletePVar(playerid, "PlayerGoingOnLine");
                }
        }
}

stock OnPlayerClickOnMetroMap(playerid, PlayerText:playertextid)
{
        if(playertextid == metro_map_PTD[playerid][29]) // закрыть ТД
        {
                DeleteMetroMapTextdraw(playerid);
                DeleteMetroStopTextdraw(playerid);
                DeletePVar(playerid, "PlayerSelectTD");
                DeletePVar(playerid, "MetroStation");
        }
        if(playertextid == metro_map_PTD[playerid][3]) // Unity 
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 2..7: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][4]) // Airport
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 3..8: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][5]) // Pershing Square
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 4..10: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][6]) // Verona Beach
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 5..11: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][7]) // Santa Maria Beach
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 6..11: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][8]) // Rodeo
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 7..12: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][9]) // Market Square
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 8..13: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][10]) // Commercial Area
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 1,9..13: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][11]) // Glen Park
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 1,2,10..13: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][12]) // Jefferson
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 1..3,11..13: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][13]) // East Side
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 1..4,12,13: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][14]) // LS Forum
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 1..5,13: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
        if(playertextid == metro_map_PTD[playerid][15]) // Willowfield
        {
                new metro_station_num = GetPVarInt(playerid, "MetroStation");
                switch(metro_station_num)
                {
                        case 1..6: SetPVarInt(playerid, "PlayerGoingOnLine", 1);
                        default: SetPVarInt(playerid, "PlayerGoingOnLine", 2);
                }
                OnPlayerSelectStation(playerid, metro_station_num);
                DeleteMetroMapTextdraw(playerid);
        }
}

stock OnPlayerSelectStation(playerid, metro_station_num)
{
        new metro_player_on_line = GetPVarInt(playerid, "PlayerGoingOnLine");
        switch(metro_player_on_line)
        {
                case 1: // против часовой стрелки
                {
                        switch(metro_station_num)
                        {
                                case 1: PlayerTextDrawSetString(
                                        playerid, metro_stop_PTD[playerid][5], metro_name[12]);
                                case 2..13: PlayerTextDrawSetString(
                                        playerid, metro_stop_PTD[playerid][5], metro_name[metro_station_num-2]);
                        }
                }
                case 2: // по часовой стрелке
                {
                        switch(metro_station_num)
                        {
                                case 1..12: PlayerTextDrawSetString(
                                        playerid, metro_stop_PTD[playerid][5], metro_name[metro_station_num]);
                                case 13: PlayerTextDrawSetString(
                                        playerid, metro_stop_PTD[playerid][5], metro_name[0]);
                        }
                }
        }
        for(new i; i < sizeof(metro_stop_PTD[]); i++)
                PlayerTextDrawShow(playerid, metro_stop_PTD[playerid][i]);
        SetPlayerPos(playerid, 3584.7244, 541.0466, 1519.5316);
        SetPlayerFacingAngle(playerid, 270.0);
        TogglePlayerControllable(playerid, false);
        ApplyAnimation(playerid, "ped", "SEAT_idle" ,4.1,1,0,0,0,0,0);
        SetPlayerCameraPos(playerid, 3586.4244, 541.0466, 1520.0316);
        SetPlayerCameraLookAt(playerid, 3584.7244, 541.0466, 1519.5316);
        GivePlayerMoney(playerid, -METRO_TICKET_PRICE);
        SetPlayerVirtualWorld(playerid, playerid+1);
        SetPVarInt(playerid, "MetroStop", 1);
        DeletePVar(playerid, "PlayerSelectTD");
        metro_timer[playerid] = SetTimerEx("MetroStantionStop", METRO_TIME_STOP, false, "d", playerid);
}

forward MetroStantionStop(playerid);
public MetroStantionStop(playerid)
{
        DeletePVar(playerid, "MetroStop");
        KillTimer(metro_timer[playerid]);
        metro_timer[playerid] = -1;
        PlayerTextDrawHide(playerid, metro_stop_PTD[playerid][7]);
        PlayerTextDrawHide(playerid, metro_stop_PTD[playerid][8]);
        PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][7], "ожидайте_прибытия_поезда");
        PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][8], "на_станцию");
        PlayerTextDrawShow(playerid, metro_stop_PTD[playerid][7]);
        PlayerTextDrawShow(playerid, metro_stop_PTD[playerid][8]);
        new metro_player_on_line = GetPVarInt(playerid, "PlayerGoingOnLine");
        new metro_station_num = GetPVarInt(playerid, "MetroStation");
        switch(metro_player_on_line)
        {
                case 1: // против часовой стрелки
                {
                        switch(metro_station_num)
                        {
                                case 1:
                                {
                                        SetPVarInt(playerid, "MetroStation", metro_station_num +12);
                                        metro_timer[playerid] = SetTimerEx(
                                                "MetroStantionOnGoing", METRO_TIME_DELAY, false, "d", playerid);
                                        return false;
                                }
                                case 2..13:
                                {
                                        SetPVarInt(playerid, "MetroStation", metro_station_num -1);
                                        metro_timer[playerid] = SetTimerEx(
                                                "MetroStantionOnGoing", METRO_TIME_DELAY, false, "d", playerid);
                                        return false;
                                }
                        }
                }
                case 2: // по часовой стрелки
                {
                        switch(metro_station_num)
                        {
                                case 1..13: 
                                {
                                        SetPVarInt(playerid, "MetroStation", metro_station_num +1);
                                        metro_timer[playerid] = SetTimerEx(
                                                "MetroStantionOnGoing", METRO_TIME_DELAY, false, "d", playerid);
                                        return false;
                                }
                        }
                }
        }
        return true;
}

forward MetroStantionOnGoing(playerid);
public MetroStantionOnGoing(playerid)
{
        KillTimer(metro_timer[playerid]);
        metro_timer[playerid] = -1;
        SetPVarInt(playerid, "MetroStop", 1);
        PlayerTextDrawHide(playerid, metro_stop_PTD[playerid][3]);
        PlayerTextDrawHide(playerid, metro_stop_PTD[playerid][5]);
        PlayerTextDrawHide(playerid, metro_stop_PTD[playerid][7]);
        PlayerTextDrawHide(playerid, metro_stop_PTD[playerid][8]);
        PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][7], "нажмите__'n'");
        PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][8], "если_хотите_выйти_здесь");
        new metro_player_on_line = GetPVarInt(playerid, "PlayerGoingOnLine");
        new metro_station_num = GetPVarInt(playerid, "MetroStation");
        switch(metro_player_on_line)
        {
                case 1:
                {
                        switch(metro_station_num)  // против часовой стрелки
                        {
                                case 1: PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][5], metro_name[12]),
                                        PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][3], metro_name[metro_station_num -1]);
                                case 2..13: PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][5], metro_name[metro_station_num-2]),
                                        PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][3], metro_name[metro_station_num-1]);

                        }
                }
                case 2:
                {
                        switch(metro_station_num)  // по часовой стрелки
                        { 
                                case 1..12: PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][5], metro_name[metro_station_num]),
                                        PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][3], metro_name[metro_station_num -1]);
                                case 13: PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][5], metro_name[0]),
                                        PlayerTextDrawSetString(playerid, metro_stop_PTD[playerid][3], metro_name[metro_station_num -1]);

                        }
                }
        }
        PlayerTextDrawShow(playerid, metro_stop_PTD[playerid][3]);
        PlayerTextDrawShow(playerid, metro_stop_PTD[playerid][5]);
        PlayerTextDrawShow(playerid, metro_stop_PTD[playerid][7]);
        PlayerTextDrawShow(playerid, metro_stop_PTD[playerid][8]);
        metro_timer[playerid] = SetTimerEx("MetroStantionStop", METRO_TIME_STOP, false, "d", playerid);
        return true;
}

stock CreateMetroMapTextdraw(playerid)
{
        metro_map_PTD[playerid][0] = CreatePlayerTextDraw(playerid, 295.0000, 90.0000, "Box"); // пусто
        PlayerTextDrawLetterSize(playerid, metro_map_PTD[playerid][0], 0.0000, 35.0000);
        PlayerTextDrawTextSize(playerid, metro_map_PTD[playerid][0], 635.0000, 0.0000);
        PlayerTextDrawAlignment(playerid, metro_map_PTD[playerid][0], 1);
        PlayerTextDrawColor(playerid, metro_map_PTD[playerid][0], -1);
        PlayerTextDrawUseBox(playerid, metro_map_PTD[playerid][0], 1);
        PlayerTextDrawBoxColor(playerid, metro_map_PTD[playerid][0], -801552641);
        PlayerTextDrawBackgroundColor(playerid, metro_map_PTD[playerid][0], 255);
        PlayerTextDrawFont(playerid, metro_map_PTD[playerid][0], 1);
        PlayerTextDrawSetProportional(playerid, metro_map_PTD[playerid][0], 1);
        PlayerTextDrawSetShadow(playerid, metro_map_PTD[playerid][0], 0);

        metro_map_PTD[playerid][1] = CreatePlayerTextDraw(playerid, 297.0000, 110.0000, "samaps:gtasamapbit4"); // пусто
        PlayerTextDrawTextSize(playerid, metro_map_PTD[playerid][1], 336.0000, 292.0000);
        PlayerTextDrawAlignment(playerid, metro_map_PTD[playerid][1], 1);
        PlayerTextDrawColor(playerid, metro_map_PTD[playerid][1], -1);
        PlayerTextDrawBackgroundColor(playerid, metro_map_PTD[playerid][1], 255);
        PlayerTextDrawFont(playerid, metro_map_PTD[playerid][1], 4);
        PlayerTextDrawSetProportional(playerid, metro_map_PTD[playerid][1], 0);
        PlayerTextDrawSetShadow(playerid, metro_map_PTD[playerid][1], 0);

        metro_map_PTD[playerid][2] = CreatePlayerTextDraw(playerid, 301.0000, 90.0000, "метрополитен_города_лос_сантос"); // пусто
        PlayerTextDrawLetterSize(playerid, metro_map_PTD[playerid][2], 0.3919, 1.7057);
        PlayerTextDrawAlignment(playerid, metro_map_PTD[playerid][2], 1);
        PlayerTextDrawColor(playerid, metro_map_PTD[playerid][2], -1);
        PlayerTextDrawSetOutline(playerid, metro_map_PTD[playerid][2], 1);
        PlayerTextDrawBackgroundColor(playerid, metro_map_PTD[playerid][2], 255);
        PlayerTextDrawFont(playerid, metro_map_PTD[playerid][2], 2);
        PlayerTextDrawSetProportional(playerid, metro_map_PTD[playerid][2], 1);
        PlayerTextDrawSetShadow(playerid, metro_map_PTD[playerid][2], 0);

        metro_map_PTD[playerid][3] = CreatePlayerTextDraw(playerid, 497.0000, 289.0000, "hud:radar_light"); // пусто
        PlayerTextDrawTextSize(playerid, metro_map_PTD[playerid][3], 7.0000, 7.0000);
        PlayerTextDrawAlignment(playerid, metro_map_PTD[playerid][3], 1);
        PlayerTextDrawColor(playerid, metro_map_PTD[playerid][3], -1);
        PlayerTextDrawBackgroundColor(playerid, metro_map_PTD[playerid][3], 255);
        PlayerTextDrawFont(playerid, metro_map_PTD[playerid][3], 4);
        PlayerTextDrawSetProportional(playerid, metro_map_PTD[playerid][3], 0);
        PlayerTextDrawSetShadow(playerid, metro_map_PTD[playerid][3], 0);
        PlayerTextDrawSetSelectable(playerid, metro_map_PTD[playerid][3], true);

        metro_map_PTD[playerid][4] = CreatePlayerTextDraw(playerid, 471.0000, 328.0000, "hud:radar_light"); // пусто
        PlayerTextDrawTextSize(playerid, metro_map_PTD[playerid][4], 7.0000, 7.0000);
        PlayerTextDrawAlignment(playerid, metro_map_PTD[playerid][4], 1);
        PlayerTextDrawColor(playerid, metro_map_PTD[playerid][4], -1);
        PlayerTextDrawBackgroundColor(playerid, metro_map_PTD[playerid][4], 255);
        PlayerTextDrawFont(playerid, metro_map_PTD[playerid][4], 4);
        PlayerTextDrawSetProportional(playerid, metro_map_PTD[playerid][4], 0);
        PlayerTextDrawSetShadow(playerid, metro_map_PTD[playerid][4], 0);
        PlayerTextDrawSetSelectable(playerid, metro_map_PTD[playerid][4], true);

        metro_map_PTD[playerid][5] = CreatePlayerTextDraw(playerid, 454.0000, 269.0000, "hud:radar_light"); // пусто
        PlayerTextDrawTextSize(playerid, metro_map_PTD[playerid][5], 7.0000, 7.0000);
        PlayerTextDrawAlignment(playerid, metro_map_PTD[playerid][5], 1);
        PlayerTextDrawColor(playerid, metro_map_PTD[playerid][5], -1);
        PlayerTextDrawBackgroundColor(playerid, metro_map_PTD[playerid][5], 255);
        PlayerTextDrawFont(playerid, metro_map_PTD[playerid][5], 4);
        PlayerTextDrawSetProportional(playerid, metro_map_PTD[playerid][5], 0);
        PlayerTextDrawSetShadow(playerid, metro_map_PTD[playerid][5], 0);
        PlayerTextDrawSetSelectable(playerid, metro_map_PTD[playerid][5], true);

