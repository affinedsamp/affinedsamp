        metro_stop_PTD[playerid][7] = CreatePlayerTextDraw(playerid, 106.0000, 398.0000, "нажмите__'n'"); // пусто
        PlayerTextDrawLetterSize(playerid, metro_stop_PTD[playerid][7], 0.2634, 1.7493);
        PlayerTextDrawAlignment(playerid, metro_stop_PTD[playerid][7], 2);
        PlayerTextDrawColor(playerid, metro_stop_PTD[playerid][7], -1);
        PlayerTextDrawSetOutline(playerid, metro_stop_PTD[playerid][7], 1);
        PlayerTextDrawBackgroundColor(playerid, metro_stop_PTD[playerid][7], 255);
        PlayerTextDrawFont(playerid, metro_stop_PTD[playerid][7], 2);
        PlayerTextDrawSetProportional(playerid, metro_stop_PTD[playerid][7], 1);
        PlayerTextDrawSetShadow(playerid, metro_stop_PTD[playerid][7], 0);

        metro_stop_PTD[playerid][8] = CreatePlayerTextDraw(playerid, 106.0000, 413.0000, "если_хотите_выйти_здесь"); // пусто
        PlayerTextDrawLetterSize(playerid, metro_stop_PTD[playerid][8], 0.2634, 1.7493);
        PlayerTextDrawAlignment(playerid, metro_stop_PTD[playerid][8], 2);
        PlayerTextDrawColor(playerid, metro_stop_PTD[playerid][8], -1);
        PlayerTextDrawSetOutline(playerid, metro_stop_PTD[playerid][8], 1);
        PlayerTextDrawBackgroundColor(playerid, metro_stop_PTD[playerid][8], 255);
        PlayerTextDrawFont(playerid, metro_stop_PTD[playerid][8], 2);
        PlayerTextDrawSetProportional(playerid, metro_stop_PTD[playerid][8], 1);
        PlayerTextDrawSetShadow(playerid, metro_stop_PTD[playerid][8], 0);
        }

stock DeleteMetroStopTextdraw(playerid)
{
        for(new i; i < sizeof(metro_stop_PTD[]); i++)
                PlayerTextDrawDestroy(playerid, metro_stop_PTD[playerid][i]);
}

